<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cafe94.OrderController">
   <children>
      <Label layoutX="136.0" layoutY="71.0" text="Choose Your Order Type">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <HBox layoutX="57.0" layoutY="117.0" prefHeight="166.0" prefWidth="485.0">
         <children>
            <Button fx:id="eatIn" mnemonicParsing="false" onAction="#getOrderType" text="Eat In">
               <font>
                  <Font size="30.0" />
               </font>
               <HBox.margin>
                  <Insets left="3.0" right="25.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="takeaway" mnemonicParsing="false" onAction="#getOrderType" text="Takeaway">
               <font>
                  <Font size="30.0" />
               </font>
               <HBox.margin>
                  <Insets right="25.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="delivery" mnemonicParsing="false" onAction="#getOrderType" text="Delivery">
               <font>
                  <Font size="30.0" />
               </font>
               <HBox.margin>
                  <Insets top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
